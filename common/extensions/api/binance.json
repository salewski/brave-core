[
  {
    "namespace": "binance",
    "description": "Use the <code>chrome.binance</code> API to receive notifications about Dapp detection.",
    "compiler_options": {
      "implemented_in": "brave/browser/extensions/api/binance_api.h"
    },
    "events": [
    ],
    "functions": [
      {
        "name": "getUserTLD",
        "type": "function",
        "description": "Gets the appropriate Binance TLD per a user's region",
        "parameters": [
          {
            "type": "function",
            "name": "callback",
            "parameters": [
              {
                "name": "userTLD",
                "type": "string"
              }
            ]
          }
        ]
      },
      {
        "name": "getClientUrl",
        "type": "function",
        "description": "Fetches the Oauth Url for Binance",
        "parameters": [
          {
            "type": "function",
            "name": "callback",
            "parameters": [
              {
                "name": "clientUrl",
                "type": "string"
              }
            ]
          }
        ]
      },
      {
        "name": "getAccessToken",
        "type": "function",
        "description": "Fetches the Oauth Url for Binance",
        "parameters": [
          {
            "type": "string",
            "name": "code"
          },
          {
            "type": "function",
            "name": "callback",
            "parameters": [
              {
                "name": "success",
                "type": "boolean"
              }
            ]
          }
        ]
      },
      {
        "name": "setCodeChallenge",
        "type": "function",
        "description": "Sets the code challenge.",
        "parameters": [
          {
            "type": "string",
            "name": "challenge"
          },
          {
            "type": "function",
            "name": "callback",
            "parameters": [
              {
                "name": "success",
                "type": "boolean"
              }
            ]
          }
        ]
      },
      {
        "name": "getAccountBalances",
        "type": "function",
        "description": "Retrieves user asset balances",
        "parameters": [
          {
            "type": "function",
            "name": "callback",
            "parameters": [
              {
                "name": "balances",
                "type": "object",
                "additionalProperties": { "type": "string" }
              }, {
                "name": "success",
                "type": "boolean",
                "description": "Indicates whether the retrieval was successful"
              }
            ]
          }
        ]
      }
    ],
    "types": [
    ],
    "properties": {
    }
  }
]
